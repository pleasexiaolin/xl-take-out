<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xiaolin.mapper.SetmealDishMapper">
    <delete id="batchDeleteBySetmealId">
        delete
        from setmeal_dish where setmeal_id in
        <foreach item="setmealId" collection="setmealIds" separator="," open="(" close=")">
            #{setmealId}
        </foreach>
    </delete>

    <delete id="deleteBySetmealId">
        delete
        from setmeal_dish
        where setmeal_id = #{setmealId}
    </delete>
</mapper>