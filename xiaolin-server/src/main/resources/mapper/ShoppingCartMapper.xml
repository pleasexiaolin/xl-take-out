<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xiaolin.mapper.ShoppingCartMapper">

    <select id="list" resultType="com.xiaolin.vo.ShoppingCartVO">
        select id,
               name,
               image,
               user_id,
               dish_id,
               setmeal_id,
               dish_flavor,
               number,
               amount,
               create_time
        from shopping_cart
    </select>
</mapper>